{
  "name": "dat",
  "version": "7.0.1",
  "description": "versioning and sync tool for data sets",
  "keywords": [
    "data package",
    "dataset",
    "replication",
    "synchronization",
    "versioning"
  ],
  "bin": {
    "dat": "cli.js"
  },
  "scripts": {
    "install-precommit": "echo ./node_modules/.bin/standard > .git/hooks/pre-commit && chmod +x .git/hooks/pre-commit",
    "pretty-test": "npm run tape | tap-spec",
    "standard": "standard",
    "tape": "tape tests/*.js",
    "test": "standard && npm run tape"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/maxogden/dat.git"
  },
  "author": "max ogden",
  "license": "BSD-3-Clause",
  "dependencies": {
    "JSONStream": "^0.10.0",
    "byte-stream": "^2.1.0",
    "cli-prompt": "^0.4.1",
    "concat-stream": "^1.5.0",
    "csv-parser": "^1.5.2",
    "cuid": "^1.2.4",
    "dat-core": "^4.0.3",
    "debug": "^2.1.3",
    "debug-stream": "^2.1.0",
    "detect-csv": "^1.0.1",
    "detect-json-style": "^1.0.2",
    "diffs-to-string": "^4.1.0",
    "format-data": "^2.1.2",
    "getport": "^0.1.0",
    "got": "^3.3.1",
    "manual-merge-stream": "^3.2.2",
    "ndjson": "^1.3.0",
    "peek-stream": "^1.1.1",
    "pretty-bytes": "^2.0.1",
    "pump": "^1.0.0",
    "pumpify": "^1.3.3",
    "read": "^1.0.6",
    "relative-date": "^1.1.2",
    "resolve": "^1.1.6",
    "rimraf": "^2.3.2",
    "single-line-log": "^1.0.0",
    "subcommand": "^2.0.1",
    "through2": "^0.6.3",
    "transport-stream": "^1.4.2"
  },
  "bugs": {
    "url": "https://github.com/maxogden/dat/issues"
  },
  "devDependencies": {
    "mkdirp": "^0.5.0",
    "npm-execspawn": "^1.2.0",
    "run-parallel": "^1.1.1",
    "sort-stream": "^1.0.1",
    "standard": "^3.3.2",
    "stream-iterate": "^1.1.1",
    "tap-spec": "^3.0.0",
    "tape": "^4.0.0",
    "tape-spawn": "^1.3.4"
  },
  "homepage": "https://github.com/maxogden/dat",
  "directories": {
    "test": "tests"
  }
}
